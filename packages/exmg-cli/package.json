{
  "name": "@exmg/exmg-cli",
  "version": "2.0.2",
  "description": "CLI - scripts used to process and build polymer /  lit-element packages",
  "contributors": [
    "Ex Machina"
  ],
  "keywords": [
    "web-components",
    "polymer",
    "lit-element",
    "cli"
  ],
  "main": "src/sass-render/index.js",
  "bin": {
    "exmg-cli-sass-render": "./bin/exmg-cli-sass-render"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.tpl",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ExmgElements/exmg-cli.git"
  },
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.0.2",
    "command-line-usage": "^6.0.2",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-watch": "^5.0.1",
    "node-sass-import": "^2.0.1",
    "replace": "^1.1.0",
    "sass": "^1.17.2"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/glob": "^7.1.1",
    "@types/gulp": "^4.0.6",
    "@types/gulp-watch": "^4.1.34",
    "@types/node": "^11.11.3",
    "@types/sass": "^1.16.0",
    "eslint": "^6.5.1",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "package-install": "npm i",
    "start": "gulp serve",
    "build": "npm run lint && npm run tsc",
    "lint": "echo No lint.",
    "tsc": "tsc -b ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "publishDryRun": "npm publish --dry-run",
    "preversion": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  }
}
