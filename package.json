{
  "name": "@exmg/exmg-paper-combobox",
  "flat": true,
  "version": "3.0.4",
  "description": "Element to parse combobox input",
  "contributors": [
    "Ex Machina Group"
  ],
  "keywords": [
    "web-components",
    "polymer",
    "typescript",
    "paper",
    "combobox"
  ],
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:ExmgElements/exmg-paper-combobox.git"
  },
  "license": "MIT",
  "main": "exmg-paper-combobox.js",
  "files": [
    "typings/*",
    "exmg-*.js",
    "exmg-*.d.ts"
  ],
  "dependencies": {
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-input": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/paper-input": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "lit-element": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/test-fixture": "^4.0.2",
    "@types/polymer": "^1.2.6",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "tslint": "^5.12.1",
    "tslint-config-exmg": "^1.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4",
    "wct-mocha": "^1.0.0",
    "web-animations-js": "^2.3.1"
  },
  "scripts": {
    "gen-docs": "typedoc --excludeNotExported --excludeExternals --excludePrivate --excludeProtected --ignoreCompilerErrors --exclude '{**/*test*,**/node_modules/**,**/test/**}' --out docs/ *.ts",
    "build": "npm run lint && npm run tsc",
    "tsc": "tsc -b",
    "prepare": "npm run build",
    "start": "gulp serve",
    "lint": "polymer lint && tslint -c ./tslint.json --project ./",
    "test-dev": "polymer test -l chrome -p",
    "test2": "polymer test",
    "test2:integration": "polymer build # test that psk builds without error with the CLI"
  }
}
