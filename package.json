{
  "name": "@exmg/exmg-cms-breadcrumbs",
  "flat": true,
  "version": "1.0.0",
  "description": "Element to display breadcrumbs navigation",
  "main": "index.js",
  "contributors": [
    "Ex Machina Group"
  ],
  "keywords": [
    "web-components",
    "polymer",
    "typescript",
    "lit-elemnt",
    "breadcrumbs"
  ],
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:exmachina/exmg-cms-breadcrumbs.git"
  },
  "license": "MIT",
  "homepage": "https://bitbucket.org/exmachina/exmg-cms-breadcrumbs",
  "files": [
    "typings/*.d.ts",
    "src/**/*.js",
    "src/**/*.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@polymer/test-fixture": "^4.0.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/polymer": "^1.2.6",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "tslint": "^5.12.1",
    "tslint-config-exmg": "^1.0.2",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333",
    "wct-mocha": "^1.0.0"
  },
  "scripts": {
    "gen-docs": "typedoc --excludeNotExported --excludeExternals --excludePrivate --excludeProtected --ignoreCompilerErrors --exclude '{**/*test*,**/node_modules/**,**/test/**}' --out docs/ **/*.ts",
    "build": "npm run lint && npm run tsc",
    "tsc": "tsc -b ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "publish-dry-run": "npm publish --dry-run",
    "start": "gulp serve",
    "lint-src": "tslint -c ./tslint.json --project ./",
    "lint-test": "tslint -c ./test/tslint.json --project ./",
    "lint": "npm run lint-src && npm run lint-test",
    "test-dev": "polymer test --npm -l chrome -p",
    "test": "polymer test --npm --expanded",
    "test:integration": "polymer build # test that psk builds without error with the CLI"
  }
}
